<Ui xmlns="http://www.blizzard.com/wow/ui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">

  <Frame name="ArenaLiveCrowdControlIconTemplate" virtual="true">
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentTexture" parentKey="texture"
          setAllPoints="true"/>
      </Layer>
    </Layers>
    <Frames>
      <Cooldown name="$parentCooldown" parentKey="cd" reverse="true"
          setAllPoints="true">
        <SwipeTexture>
          <Color r="0" g="0" b="0" a="0.8" />
        </SwipeTexture>
        <EdgeTexture file="Interface\Cooldown\edge" />
      </Cooldown>
    </Frames>
  </Frame>
</Ui>
