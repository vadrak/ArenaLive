<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

  <Button name="ArenaLiveUnitFrameTemplate" inherits="DeliUnitFrameTemplate"
      virtual="true">
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentBackground" file="Interface\AddOns\ArenaLive\textures\SideFrameBackground">
          <TexCoords left="0.140625" right="0.8515625" top="0.20703125"
              bottom="0.76953125"/>
        </Texture>
      </Layer>
    </Layers>
    <Frames>
      <!-- We nest the frames so many times due to FrameLevel. -->
      <Frame setAllPoints="true">
        <Frames>
          <Frame setAllPoints="true">
            <Frames>
              <Frame setAllPoints="true">
                <Frames>
                  <Frame setAllPoints="true">
                    <Layers>
                      <Layer level="BACKGROUND">
                        <Texture name="$parentBorder" file="Interface\AddOns\ArenaLive\textures\SideFrameBorder">
                          <Size>
                            <AbsDimension x="184" y="75" />
                          </Size>
                          <Anchors>
                            <Anchor point="TOPLEFT" />
                          </Anchors>
                          <TexCoords left="0.140625" right="0.859375"
                              top="0.20703125" bottom="0.7890625"/>
                        </Texture>
                      </Layer>
                    </Layers>
                  </Frame>
                </Frames>
              </Frame>
            </Frames>
          </Frame>
        </Frames>
      </Frame>
    </Frames>
  </Button>
</Ui>
