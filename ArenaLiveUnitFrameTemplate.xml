<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

  <Button name="ArenaLiveLeftUnitFrameTemplate"
      inherits="DeliUnitFrameTemplate" virtual="true">
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentClassIconBackground">
          <Size x="68" y="68" />
          <Anchors>
            <Anchor point="TOPLEFT" />
          </Anchors>
          <Color r="0" g="0" b="" a="0.5" />
        </Texture>
        <Texture name="$parentHealthBarBackground">
          <Size x="132" y="42" />
          <Anchors>
            <Anchor point="TOPLEFT" relativeTo="$parentClassIconBackground"
                relativePoint="TOPRIGHT">
              <Offset>
                <AbsDimension x="2" y="0" />
              </Offset>
            </Anchor>
          </Anchors>
          <Color r="0" g="0" b="" a="0.5" />
        </Texture>
        <Texture name="$parentPowerBarBackground">
          <Size x="132" y="24" />
          <Anchors>
            <Anchor point="TOPLEFT" relativeTo="$parentHealthBarBackground"
                relativePoint="BOTTOMLEFT">
              <Offset>
                <AbsDimension x="0" y="-2" />
              </Offset>
            </Anchor>
          </Anchors>
          <Color r="0" g="0" b="" a="0.5" />
        </Texture>
      </Layer>
    </Layers>
    <Frames>
      <Frame setAllPoints="true">
        <Frames>
          <Frame setAllPoints="true">
            <Layers>
              <Layer level="OVERLAY">
                <Texture name="$parentClassIconOverlay" file="Interface\AddOns\ArenaLive\textures\ClassIconOverlay">
                  <Size>
                    <AbsDimension x="64" y="64" />
                  </Size>
                  <Anchors>
                    <Anchor point="TOPLEFT">
                      <Offset>
                        <AbsDimension x="2" y="-2" />
                      </Offset>
                    </Anchor>
                  </Anchors>
                </Texture>
              </Layer>
            </Layers>
          </Frame>
        </Frames>
      </Frame>
    </Frames>
  </Button>

